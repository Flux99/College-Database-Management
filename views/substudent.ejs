<%- include('partials/adminheader') %>
<div class="main">

  <table class="striped">
          <thead>
            <tr>
                <th> ID</th>
                <th> Name</th>
                <th>SubjectCode</th>
                <th>Semester</th>
                <th>Teacher ID</th>
                <th>Year</th>


            </tr>
          </thead>

          <tbody>

            <%
                   if(userData.length!=0){

                   userData.forEach(function(data){
                   %>
                   <tr>
                      <td><%=data.id; %></td>
                      <td><%=data.fullname %></td>
                       <td><%=data.subjectcode %></td>
                       <td><%=data.Sem %></td>
                       <td><%=data.teacherID; %></td>
                       <td><%=data.Year %></td>
                       <td>
                         <a class="waves-effect waves-light btn red" href="/studentsub/<%=data.id%>"><i class="material-icons">delete</i></a>
                        </td>
                   <%   }) %>

                   <% } else{ %>
                      </tr>
                       <tr>
                           <td colspan="7">No Data Found</td>
                       </tr>
                   <% } %>


            <!-- <tr>
              <td>Alvin</td>
              <td>Eclair</td>
              <td>$0.87</td>
            </tr> -->

          </tbody>
        </table>



</div>


<%- include('partials/adminfooter') %>
