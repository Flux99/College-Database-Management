<%- include('partials/header') -%>

<form class="" action="/second" method="post">


<div class="row main">


<div class="row">
          <div class="input-field col s6">
            <input id="aadhar_number" name="aadharnumber" type="text" data-length="12">
            <label for="aadhar_number">Enter your Aadharcard</label>
          </div>
        </div>

<div class="row">

  <div class="input-field col s6" required="" aria-required="true">
    <select name="selectpickerI">
      <option value="" disabled selected>Choose your option</option>
      <option value="0">0</option>
      <option value="1">1</option>
      <option value="2">2</option>
      <option value="3">3</option>
    </select>
    <label>Sem I Any KT </label>
  </div>

  <div class="input-field col s6">
    <input id="sem1cgpa" name="sem1cgpa" type="number" min="1" max="100" step="0.01" class="validate" required="" aria-required="true">
    <label for="sem1cgpa">CGPA</label>
  </div>

</div>

<div class="row">

  <div class="input-field col s6" required="" aria-required="true">
    <select name="selectpickerII">
      <option value="" disabled selected>Choose your option</option>
      <option value="0">0</option>
      <option value="1">1</option>
      <option value="2">2</option>
      <option value="3">3</option>
    </select>
    <label>Sem II Any KT </label>
  </div>

  <div class="input-field col s6">
    <input id="sem2cgpa" name="sem2cgpa" type="number" min="1" max="100" step="0.01" class="validate" required="" aria-required="true">
    <label for="sem2cgpa">CGPA</label>
  </div>

</div>
<div class="row">
  <button class="btn waves-effect waves-light" type="submit" name="action">Submit
  </button>
</div>
</div>
</form>
